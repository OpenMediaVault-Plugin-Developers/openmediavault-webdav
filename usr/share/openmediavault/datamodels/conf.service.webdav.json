{
    "type": "config",
    "id": "conf.service.webdav",
    "title": "WebDAV",
    "queryinfo": {
        "xpath": "/config/services/webdav",
        "iterable": false
    },
    "properties": {
        "enable": {
            "type": "boolean",
            "default": false
        },
        "sharedfolderref": {
            "type": "string",
            "oneOf": [{
                "type": "string",
                "format": "uuidv4"
            }, {
                "type": "string",
                "maxLength": 0
            }],
            "default": ""
        },
        "upload_max": {
            "type": "integer",
            "minimum": 1,
            "default": true
        }
    }
}
